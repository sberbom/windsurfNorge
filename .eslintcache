\r\n\r\n  return <div className=\"map\" id={mapRef}/>;\r\n}\r\n\r\nexport default SBMap;\r\n","/home/sberbom/code_projects/windsurfnorge/src/components/title.js",[],"/home/sberbom/code_projects/windsurfnorge/src/components/cardList.js",[],"/home/sberbom/code_projects/windsurfnorge/src/components/sortbar.js",[],"/home/sberbom/code_projects/windsurfnorge/src/components/spotInfo.js",[],"/home/sberbom/code_projects/windsurfnorge/src/components/weather.js",[],"/home/sberbom/code_projects/windsurfnorge/src/components/imageGallery.js",[],"/home/sberbom/code_projects/windsurfnorge/src/components/emailVerificationModal.js",[],"/home/sberbom/code_projects/windsurfnorge/src/utils.js",[],"/home/sberbom/code_projects/windsurfnorge/src/components/myContent.js",[],"/home/sberbom/code_projects/windsurfnorge/src/components/myinfo.js",[],"/home/sberbom/code_projects/windsurfnorge/src/components/registerinfo.js",[],"/home/sberbom/code_projects/windsurfnorge/src/components/input.js",[],"/home/sberbom/code_projects/windsurfnorge/src/components/deleteSpotModal.js",[],"/home/sberbom/code_projects/windsurfnorge/src/components/weatherCard.js",[],"/home/sberbom/code_projects/windsurfnorge/src/components/spotCreatedByUserEntry.js",[],"/home/sberbom/code_projects/windsurfnorge/src/components/reAuthenticateModal.js",[],"/home/sberbom/code_projects/windsurfnorge/src/components/imageUploade.js",[],"/home/sberbom/code_projects/windsurfnorge/src/components/imageSelector.js",[],"/home/sberbom/code_projects/windsurfnorge/src/components/confirmDeleteModal.js",[],"/home/sberbom/code_projects/windsurfnorge/src/keys.js",[],"/home/sberbom/code_projects/windsurfnorge/src/components/footer.tsx",[],"/home/sberbom/code_projects/windsurfnorge/src/components/header.tsx",[],"/home/sberbom/code_projects/windsurfnorge/src/providers/userProvider.tsx",[],"/home/sberbom/code_projects/windsurfnorge/src/types/types.tsx",[],"/home/sberbom/code_projects/windsurfnorge/src/components/addSpotForm.tsx",[],"/home/sberbom/code